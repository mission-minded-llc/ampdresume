{
  "name": "openresume",
  "version": "1.0.0",
  "description": "Uniform standard for resume data.",
  "private": true,
  "type": "module",
  "keywords": [
    "resume"
  ],
  "author": "Michael R. Dinerstein",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/missionmike/openresume/issues"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check": "npm run types:check && npm run lint && npm run prettier && npm run test",
    "lint": "next lint",
    "prettier": "prettier --check .",
    "types:check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seed/all.ts",
    "rds:start:test": "./scripts/rds.sh -start:test",
    "rds:stop:test": "./scripts/rds.sh -stop:test",
    "rds:start:prod": "./scripts/rds.sh -start:prod",
    "rds:stop:prod": "./scripts/rds.sh -stop:prod",
    "ssm:start": "./scripts/ssm.sh -start",
    "ssm:connect:test": "./scripts/ssm.sh -connect:test",
    "ssm:connect:prod": "./scripts/ssm.sh -connect:prod",
    "ssm:stop": "./scripts/ssm.sh -stop",
    "terraform:validate": "cd terraform && terraform validate",
    "terraform:plan": "cd terraform && terraform plan",
    "terraform:apply": "cd terraform && terraform apply"
  },
  "dependencies": {
    "@apollo/client": "^3.11.10",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@iconify/react": "^5.0.2",
    "@lexical/react": "^0.21.0",
    "@mui/base": "^5.0.0-beta.68",
    "@mui/icons-material": "^6.2.0",
    "@mui/material": "^6.1.9",
    "@mui/material-nextjs": "^6.1.8",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/third-parties": "^15.0.3",
    "@prisma/client": "^6.0.1",
    "@tanstack/react-query": "^5.62.7",
    "classnames": "^2.5.1",
    "graphql": "^16.9.0",
    "graphql-yoga": "^5.10.4",
    "html-react-parser": "^5.2.1",
    "next": "15.0.3",
    "next-auth": "^4.24.10",
    "nodemailer": "^6.9.16",
    "react": "^18.3.1",
    "react-color": "^2.19.3",
    "react-dom": "^18.3.1",
    "sass": "^1.81.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.3.12",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^18.3.1",
    "@whatwg-node/fetch": "^0.10.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
