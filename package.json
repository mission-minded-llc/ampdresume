{
  "name": "ampdresume",
  "version": "1.1.13",
  "description": "Uniform standard for resume data.",
  "private": true,
  "keywords": [
    "resume"
  ],
  "author": "Michael R. Dinerstein",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/mission-minded-llc/ampdresume/issues"
  },
  "scripts": {
    "init": "sh ./scripts/localSetup.sh",
    "dev": "next dev",
    "prebuild": "tsx scripts/generateRobotsTxt.ts && tsx scripts/copyIconifyJson.ts && tsx scripts/copyPdfJsDist.ts",
    "build": "prisma generate && next build",
    "start": "next start",
    "check": "npm run types:check && npm run lint && npm run prettier && npm run test",
    "lint": "eslint .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "types:check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "cypress:cleanup": "tsx scripts/cleanupTestUser.ts",
    "cypress:test": "cypress run",
    "cypress:open": "cypress open",
    "cypress:upload:screenshots": "tsx scripts/uploadCypressScreenshots.ts",
    "cypress:upload:videos": "tsx scripts/uploadCypressVideos.ts",
    "copy:pdfjs": "tsx scripts/copyPdfJsDist.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seed/all.ts",
    "terraform:validate": "cd terraform && terraform validate",
    "terraform:plan": "cd terraform && terraform plan",
    "terraform:apply": "cd terraform && terraform apply"
  },
  "dependencies": {
    "@apollo/client": "^4.1.0",
    "@aws-sdk/client-s3": "^3.832.0",
    "@base-ui-components/react": "^1.0.0-beta.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@iconify/json": "^2.2.350",
    "@iconify/react": "^6.0.0",
    "@lexical/react": "^0.39.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/material-nextjs": "^7.1.1",
    "@mui/x-date-pickers": "^8.5.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/third-parties": "^16.1.4",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "@sentry/nextjs": "^10.35.0",
    "@tanstack/react-query": "^5.80.7",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "graphql": "^16.11.0",
    "graphql-yoga": "^5.13.5",
    "html-react-parser": "^5.2.5",
    "html2pdf.js": "^0.14.0",
    "next": "^16.1.4",
    "next-auth": "^4.24.11",
    "nodemailer": "^7.0.12",
    "openai": "^6.16.0",
    "pdfjs-dist": "^5.3.31",
    "pg": "^8.13.1",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-color": "^2.19.3",
    "react-dom": "^19.1.0",
    "sass": "^1.89.2",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@eslint/eslintrc": "^3.3.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.9",
    "@types/nodemailer": "^7.0.5",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.1.8",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^19.1.6",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/parser": "^8.38.0",
    "babel-jest": "^30.0.1",
    "chai": "^6.0.1",
    "cypress": "^15.9.0",
    "eslint": "^9",
    "eslint-config-next": "^16.1.4",
    "eslint-plugin-import": "^2.32.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.1",
    "jest-environment-jsdom": "^30.0.1",
    "prettier": "^3.5.3",
    "prisma": "^7.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
