{
  "name": "ampdresume",
  "version": "1.0.0",
  "description": "Uniform standard for resume data.",
  "private": true,
  "type": "module",
  "keywords": [
    "resume"
  ],
  "author": "Michael R. Dinerstein",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/mission-minded-llc/ampdresume/issues"
  },
  "scripts": {
    "init": "sh ./scripts/localSetup.sh",
    "dev": "next dev",
    "prebuild": "tsx scripts/generateRobotsTxt.ts && tsx scripts/copyIconifyJson.ts",
    "build": "prisma generate && next build",
    "start": "next start",
    "check": "npm run types:check && npm run lint && npm run prettier && npm run test",
    "lint": "next lint",
    "prettier": "prettier --check .",
    "types:check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "cypress:cleanup": "tsx scripts/cleanupTestUser.ts",
    "cypress:test": "cypress run",
    "cypress:upload:screenshots": "tsx scripts/uploadCypressScreenshots.ts",
    "cypress:upload:videos": "tsx scripts/uploadCypressVideos.ts",
    "copy:pdfjs": "tsx scripts/copyPdfJsDist.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seed/all.ts",
    "terraform:validate": "cd terraform && terraform validate",
    "terraform:plan": "cd terraform && terraform plan",
    "terraform:apply": "cd terraform && terraform apply"
  },
  "dependencies": {
    "@ampdresume/theme": "^0.1.15",
    "@apollo/client": "^3.13.8",
    "@aws-sdk/client-s3": "^3.832.0",
    "@base-ui-components/react": "^1.0.0-beta.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@iconify/json": "^2.2.350",
    "@iconify/react": "^6.0.0",
    "@lexical/react": "^0.32.1",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@mui/material-nextjs": "^7.1.1",
    "@mui/x-date-pickers": "^8.5.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/third-parties": "^15.3.4",
    "@prisma/client": "^6.10.1",
    "@sentry/nextjs": "^9.30.0",
    "@tanstack/react-query": "^5.80.7",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "graphql": "^16.11.0",
    "graphql-yoga": "^5.13.5",
    "html-react-parser": "^5.2.5",
    "html2pdf.js": "^0.10.3",
    "next": "^15.3.4",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.6.5",
    "openai": "^5.5.1",
    "pdfjs-dist": "^5.3.31",
    "react": "^19.1.0",
    "react-color": "^2.19.3",
    "react-dom": "^19.1.0",
    "sass": "^1.89.2",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.1.8",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "^19.1.6",
    "babel-jest": "^30.0.1",
    "cypress": "^14.5.0",
    "eslint": "^9",
    "eslint-config-next": "^15.3.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.1",
    "jest-environment-jsdom": "^30.0.1",
    "prettier": "^3.5.3",
    "prisma": "^6.10.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
