{
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "paths": {
      "cy/*": ["../cypress/*"]
    },
    "types": ["cypress"],
    "target": "ES5"
  }
}
